{
  "name": "bimandco_onfly",
  "version": "1.0.0",
  "description": "VS 2015 and React",
  "license": "MIT",
  "scripts": {
    "start": "npx cross-env NODE_ENV=development TARGET_ENV=dev ESLINT_NO_DEV_ERRORS=true npx webpack serve --config webpack.dev.config.js",
    "start:test": "npx cross-env NODE_ENV=development TARGET_ENV=test ESLINT_NO_DEV_ERRORS=true npx webpack serve --config webpack.dev.config.js",
    "start:preprod": "npx cross-env NODE_ENV=development TARGET_ENV=preprod ESLINT_NO_DEV_ERRORS=true npx webpack serve --config webpack.dev.config.js",
    "start:prodrc": "npx cross-env NODE_ENV=development TARGET_ENV=prodrc ESLINT_NO_DEV_ERRORS=true npx webpack serve --config webpack.dev.config.js",
    "test": "npx jest",
    "version:onfly-connect": "npm view @bim-co/onfly-connect versions --json",
    "version:design-system": "npm view @bim-co/componentui-foundation versions --json",
    "build:test": "npx cross-env NODE_ENV=production TARGET_ENV=test npx webpack --config webpack.test.config.js",
    "build:preprod": "npx cross-env NODE_ENV=production TARGET_ENV=preprod npx webpack --config webpack.preprod.config.js",
    "build:prodrc": "npx cross-env NODE_ENV=production TARGET_ENV=prodrc npx webpack --config webpack.prod.config.js",
    "build": "npx cross-env NODE_ENV=production TARGET_ENV=prod npx webpack --config webpack.prod.config.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "npx start-storybook -p 6006",
    "build-storybook": "npx build-storybook",
    "generate-dev-certs": "cd certs && mkcert \"*.dev.onfly.io\"",
    "cypress:open": "cypress open",
    "test:cypress": "cypress run",
    "test:cypress:preprod": "cypress run --env BASE_URL=https://sandbox.preprod.onfly.io/ BASE_URL_HAGER=https://hager.preprod.onfly.io/ --reporter teamcity"
  },
  "dependencies": {
    "@bim-co/componentui-foundation": "^1.7.5-next.31",
    "@bim-co/onfly-connect": "^1.5.9-1",
    "@date-io/moment": "^1.0.2",
    "@elastic/apm-rum": "^5.12.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@types/seamless-immutable": "7.1.11",
    "ahooks": "^3.3.0",
    "autosuggest-highlight": "^3.1.1",
    "buffer": "^6.0.3",
    "create-react-class": "^15.6.3",
    "crypto-browserify": "^3.12.0",
    "d3-ease": "1.0.5",
    "d3-interpolate": "1.3.2",
    "d3-selection": "1.3.2",
    "d3-shape": "1.2.2",
    "flexsearch": "^0.6.30",
    "fuse.js": "^6.6.2",
    "history": "^5.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "0.5.23",
    "qrcode.react": "^0.8.0",
    "react": "^16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.8.6",
    "react-dropzone": "^5.1.0",
    "react-hook-form": "^6.7.0",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.2.2",
    "react-string-replace": "^0.4.4",
    "react-virtualized": "^9.21.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "1.49.9",
    "seamless-immutable": "^7.1.4",
    "stream-browserify": "^3.0.0",
    "toastr": "^2.1.4",
    "underscore": "^1.9.1",
    "util": "^0.12.5",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@bim-co/eslint-config-bimandco": "1.0.1-next.10",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-select": "^3.0.10",
    "@types/uuid": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@webpack-cli/serve": "^1.5.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-emotion": "^10.0.33",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^6.2.0",
    "cypress": "^9.5.0",
    "cypress-teamcity-reporter": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^25.3.0",
    "jest-svg-transformer": "^1.0.0",
    "jest-teamcity": "^1.7.0",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "raw-loader": "^4.0.2",
    "react-dev-tools": "^0.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.2",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.8.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@emotion",
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "reporters": [
      "default",
      "jest-teamcity"
    ],
    "setupFiles": [
      "<rootDir>/tests/config.ts"
    ],
    "globals": {
      "TARGET_ENV": {
        "API_URL": "https://test.bimandco.com"
      }
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/obj",
      "<rootDir>/cypress"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/tests/styleMock.js",
      "^@reducers/(.*)": "<rootDir>/Scripts/App/Reducers/$1"
    }
  }
}
